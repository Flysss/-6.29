/**
 *  @人的数组
 */
@property (nonatomic, strong) NSArray *usercalls;
/**
 *  广告跳转的判断
 */
@property (nonatomic, strong) NSString *indexImgType;
/**
 *  广告跳转页面需要的
 */
@property (nonatomic, strong) NSString *indexImgUrl;
/**
